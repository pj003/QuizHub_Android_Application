<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.katykarthik.quizhubnew">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home"></activity>
        <activity android:name=".Start" />
        <activity android:name=".Playing" />
        <activity android:name=".Done" />
        <activity android:name=".ScoreDetail" />
        <activity android:name=".score_detail_layout" />
        <activity android:name=".Quiz"></activity>
    </application>

</manifest>